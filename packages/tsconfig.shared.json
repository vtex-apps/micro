{
  "compilerOptions": {
    "typeRoots": [
      "./framework/typings",
      "./plugins/typings",
      "./router/typings",
      "./utils/typings",
    ],
    "target": "es2019",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true,
    "declaration": true
  },
  "include": ["utils", "framework", "plugins", "router"],
  "exclude": ["dist", "components", "pages"]
}
